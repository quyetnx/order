{
  "1688": {
    "selector": {
      "wrap": ".mod-detail-purchasing",
      "skuRows": ".obj-leading, .obj-sku",
      "product": {
        "link": "",
        "image": "",
        "price": ""
      },
      "skuGroup": ".obj-title",
      "skuProperties": [
        {
          "list": ".list-leading > li",
          "object": ".unit-detail-spec-operator",
          "attributes": {
            "unitConfig": [
              "name",
              "imgs"
            ]
          }
        },
        {
          "list": ".table-sku > tbody > tr",
          "object": "this",
          "attributes": {
            "skuConfig": [
              "skuName"
            ]
          }
        }
      ],
      "cartControl": {
        "control": ".unit-detail-amount-control > a",
        "input": ".amount-input",
        "denied": ".noPost"
      },
      "btnOrder": ".unit-detail-order-action",
      "selectedClass": ".active",
      "shopLink": ".smt-info > .header > a"
    }
  },
  "tmall": {
    "selector": {
      "wrap": ".tb-sku",
      "skuRows": ".tb-prop.tm-sale-prop",
      "product": {
        "link": {
          "target": ".tb-detail-hd > h1 > a",
          "params": "id"
        },
        "image": "#J_ImgBooth",
        "price": ".tm-price"
      },
      "skuGroup": ".tb-metatit",
      "skuProperties": [
        {
          "list": "ul > li",
          "object": null,
          "attributes": null,
          "images": ["a", "style:background"]
        }
      ],
      "cartControl": {
        "control": ".tb-amount-widget > span",
        "input": ".tb-text",
        "denied": ".noPost"
      },
      "btnOrder": ".tb-action",
      "selectedClass": ".tb-selected",
      "shopLink": ".slogo-shopname"
    }
  }
}